{
    "/socket.io": {
        "target": "http://localhost:4000",
        "ws": true,
        "secure": false,
        "logLevel": "debug"
    },
    "/api": {
        "target": "http://localhost:4000",
        "secure": false,
        "changeOrigin": true,
        "withCredentials": true,
        "headers": {
            "X-Frame-Options": "SAMEORIGIN",
            "Connection": "keep-alive"
        },
        "proxyTimeout": 120000,
        "timeout": 120000
    }
}